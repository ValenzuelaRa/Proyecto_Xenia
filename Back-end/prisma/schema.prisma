// schema.prisma

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

// schema.prisma

model Laboratorios {
  id        Int      @id @default(autoincrement())
  nombre    String
}

model Carreras {
  id        Int      @id @default(autoincrement())
  nombre    String
}
